
<!DOCTYPE html>
<html>
    <head>
        
    </head>
<body>

<div class="mt-4">
    <div class="row">
      
        <h1 style="color: red">Thông số ngày {{this.Date}} </h1>
      
        <table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">STT</th>
      <th scope="col">Nhiệt độ (°C)</th>
      <th scope="col">Độ ẩm (%)</th>
      <th scope="col">Ghi chú</th>
    </tr>
  </thead>
  <tbody>
        {{#each sensors}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.temperature}}</td>
      <td>{{this.humidity}}</td>
      <td>{{this.Hour}}</td>
    </tr>
        {{/each}}

    </tr>
  </tbody>
</table>     
    </div>
</div>
</body>
</html>